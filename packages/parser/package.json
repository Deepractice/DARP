{
  "name": "@darp/parser",
  "version": "0.1.0",
  "description": "ARP (Agent Resource Protocol) parser - converts ARP strings to structured ARL objects",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "arp",
    "parser",
    "agent-resource-protocol",
    "arl",
    "ari"
  ],
  "author": "Deepractice",
  "license": "Apache-2.0",
  "devDependencies": {
    "@deepracticex/config-preset": "^0.1.0",
    "@deepracticex/vitest-cucumber": "^1.3.0",
    "@types/node": "^22.10.6",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
